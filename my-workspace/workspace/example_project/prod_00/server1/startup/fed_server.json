{
  "format_version": 2,
  "servers": [
    {
      "name": "example_project",
      "service": {
        "target": "server1:8002",
        "scheme": "grpc"
      },
      "admin_host": "server1",
      "admin_port": 8003,
      "ssl_private_key": "server.key",
      "ssl_cert": "server.crt",
      "ssl_root_cert": "rootCA.pem"
    }
  ],
  "overseer_agent": {
    "path": "nvflare.ha.dummy_overseer_agent.DummyOverseerAgent",
    "args": {
      "sp_end_point": "host.docker.internal:8002:8003"
    }
  }
}