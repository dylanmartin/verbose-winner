{
  "format_version": 2,
  "servers": [
    {
      "name": "example_project",
      "service": {
        "target": "server2:8102",
        "scheme": "grpc"
      },
      "admin_host": "server2",
      "admin_port": 8103,
      "ssl_private_key": "server.key",
      "ssl_cert": "server.crt",
      "ssl_root_cert": "rootCA.pem"
    }
  ],
  "overseer_agent": {
    "path": "nvflare.ha.overseer_agent.HttpOverseerAgent",
    "args": {
      "role": "server",
      "overseer_end_point": "https://overseer:8443/api/v1",
      "project": "example_project",
      "name": "server2",
      "fl_port": "8102",
      "admin_port": "8103"
    }
  }
}