{
  "format_version": 2,
  "servers": [
    {
      "name": "example_project",
      "service": {
        "scheme": "grpc"
      }
    }
  ],
  "client": {
    "ssl_private_key": "client.key",
    "ssl_cert": "client.crt",
    "ssl_root_cert": "rootCA.pem"
  },
  "overseer_agent": {
    "path": "nvflare.ha.overseer_agent.HttpOverseerAgent",
    "args": {
      "role": "client",
      "overseer_end_point": "https://overseer:8443/api/v1",
      "project": "example_project",
      "name": "site-2"
    }
  }
}